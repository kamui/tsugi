{
  "name": "tsugi",
  "version": "0.0.0",
  "description": "next.js bootstrap with postcss css modules",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "build": "node build_static.js && next build",
    "start": "node server.ts",
    "bundle-analyze": "BUNDLE_ANALYZE=both NODE_ENV=production yarn build",
    "bundle-analyze:browser": "BUNDLE_ANALYZE=browser NODE_ENV=production yarn build",
    "bundle-analyze:server": "BUNDLE_ANALYZE=server NODE_ENV=production yarn build",
    "test": "APP_ENV=test jest",
    "test:watch": "APP_ENV=test jest --watch",
    "test:ci": "APP_ENV=test jest --bail --ci --coverage --testResultsProcessor=jest-junit --maxWorkers 8",
    "storybook": "start-storybook -p 6006 -c .storybook -s .",
    "storybook:build": "build-storybook -c .storybook -o $PWD/docs && cp -R static docs && sed -i -e 's|/static/|./static/|g' docs/static/*.js",
    "cypress": "cypress open",
    "cypress:ci": "cypress run --spec"
  },
