{
  "name": "tsugi",
  "version": "0.0.0",
  "description": "next.js bootstrap with postcss css modules",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "dev": "nodemon",
    "build": "tsc -p tsconfig.server.json && next build",
    "start": "ts-node --project tsconfig.server.json server/index.ts",
    "bundle-analyze": "ANALYZE=true NODE_ENV=production yarn build",
    "test": "APP_ENV=test jest",
    "test:watch": "APP_ENV=test jest --watch",
    "test:ci": "APP_ENV=test jest --bail --ci --coverage --testResultsProcessor=jest-junit --maxWorkers 8",
    "test:integration": "jest -c integration/jest.config.js",
    "test:integration:watch": "jest -c integration/jest.config.js --watch",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "clean-node": "rm -rf node_modules/",
    "clean-next": "rm -rf build/ dist/"
  },
